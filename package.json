{
  "name": "@bnch/benchmarker",
  "version": "0.1.0",
  "description": "A secure JavaScript benchmarking library with web worker isolation and performance.now() precision",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "npm run build && vitest run",
    "test:unit": "npm run build && vitest run",
    "test:acceptance": "npm run build && vitest run --config vitest.acceptance.config.ts",
    "test:all": "npm run test:unit && npm run test:acceptance",
    "test:watch": "npm run build && vitest --watch",
    "test:acceptance:watch": "npm run build && vitest --watch --config vitest.acceptance.config.ts",
    "test:coverage": "npm run build && vitest --coverage",
    "test:verbose": "npm run build && cross-env VITEST_VERBOSE=true vitest run",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --fix --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js}\" \"*.{ts,js,json,md}\"",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test:unit",
    "release": "semantic-release"
  },
  "keywords": [
    "benchmark",
    "performance",
    "javascript",
    "typescript",
    "web-worker",
    "secure"
  ],
  "author": "bnch.dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cerealean/bnch-dev-benchmarker.git"
  },
  "bugs": {
    "url": "https://github.com/cerealean/bnch-dev-benchmarker/issues"
  },
  "homepage": "https://bnch.dev",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.2.2",
    "cross-env": "^10.0.0",
    "eslint": "^8.56.0",
    "jsdom": "^26.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.10.0",
    "rollup-plugin-copy": "^3.5.0",
    "semantic-release": "^23.0.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}
